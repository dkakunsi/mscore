##########################
# PLATFORM CONFIGURATION #
##########################
/platform=
/platform/mongo=
/platform/mongo/host=storage
/platform/mongo/port=27017
/platform/mongo/secure=true
/platform/mongo/username=default
/platform/mongo/password=default
/platform/mongo/database=reference
/platform/kafka=
/platform/kafka/bootstrap.servers=channel:9092
/platform/kafka/acks=1
/platform/kafka/key.serializer=org.apache.kafka.common.serialization.StringSerializer
/platform/kafka/value.serializer=org.apache.kafka.common.serialization.StringSerializer
/platform/kafka/enable.auto.commit=true
/platform/kafka/auto.commit.interval.ms=5000
/platform/kafka/key.deserializer=org.apache.kafka.common.serialization.StringDeserializer
/platform/kafka/value.deserializer=org.apache.kafka.common.serialization.StringDeserializer
/platform/kafka/publishing.delay=1000
/platform/kafka/topic.region=default.kafka.topic.region.updated
/platform/kafka/topic.people=default.kafka.topic.people.updated
/platform/kafka/topic.department=default.kafka.topic.department.updated
/platform/kafka/topic.project=default.kafka.topic.project.updated
/platform/kafka/topic.budget=default.kafka.topic.budget.updated
/platform/kafka/topic.workflow=default.kafka.topic.workflow.updated
/platform/kafka/topic.notification=default.kafka.topic.notification
/platform/kafka/topic.domain=default.kafka.topic.domain
/platform/elasticsearch=
/platform/elasticsearch/host=http://index:9200
/platform/elasticsearch/secure=true
/platform/elasticsearch/username=elastic
/platform/elasticsearch/password=default
/platform/mail=
/platform/mail/host=mail
/platform/mail/port=25
/platform/mail/from=noreply@referencesolution.com
/platform/mail/possibleAttributes=email
/platform/postgres=
/platform/postgres/host=rdbms
/platform/postgres/port=5432
/platform/postgres/username=default
/platform/postgres/password=default
/platform/keycloak=
/platform/keycloak/public.
/platform/service=
/platform/service/timezone=Asia/Makassar
/platform/service/web.port=2000
/platform/service/registry.static=false
/platform/service/registry.protocol=http
